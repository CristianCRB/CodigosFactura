  object QryActualizaEstado: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftUnknown
        Name = 'pDocumento'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'pXml'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'pCufe'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'pEstado'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
